{
    "author": "Tran Nogc Quy",
    "name": "OALD word lookup extension",
    "version": "1.0",
    "description": "Double click to lookup the word in a new tab!",
    "permissions": [
        "activeTab",
        "tabs",
        "storage"
    ],
    "options_ui": {
        "page": "options.html",
        "open_in_tab": false
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentscript.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "128": "images/icon128.png"
        },
        "default_title": "OALD lookup"
    },
    "manifest_version": 2
}